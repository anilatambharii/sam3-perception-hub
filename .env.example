# SAM3 Perception Hub Configuration
# Copy to .env and customize

# Model Settings
MODEL_CACHE_DIR=./models
SAM3_INFERENCE_PROVIDER=local
SAM3_DEVICE=cuda:0

# API Ports
PERCEPTION_API_PORT=8080
RECONSTRUCTION_API_PORT=8081
AGENT_BRIDGE_PORT=8082
PLAYGROUND_PORT=3000

# Privacy
ENABLE_FACE_BLUR=true
AUDIT_LOG_ENABLED=true

# Infrastructure
REDIS_URL=redis://localhost:6379
LOG_LEVEL=INFO
